itk_module_test()

set(ModuleTemplateTests
  RotateLabels.cxx
  )

CreateTestDriver(ModuleTemplate "${ModuleTemplate-Test_LIBRARIES}" "${ModuleTemplateTests}")

itk_add_test(NAME RotateLabelsTest
  COMMAND ModuleTemplateTestDriver
  --compare
    ${ITK_TEST_OUTPUT_DIR}/RotateLabelsTestOutput.mha
    DATA{Baseline/gl_gaussian_3.mha}
  RotateLabels
    DATA{Input/classification.mha}
    ${ITK_TEST_OUTPUT_DIR}/
    3
  )
